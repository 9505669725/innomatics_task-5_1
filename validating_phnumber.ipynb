    "import re\n",
    "num=int(input())\n",
    "pattern=r\"^[7-9]\\d{9}$\"\n",
    "for i in range(num):\n",
    "    if bool(re.search(pattern,input()))==True:\n",
    "        print('YES')\n",
    "    else:\n",
    "        print('NO')"
